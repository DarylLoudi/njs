local function teleportToNamedLocation(targetName)
    if not targetName then
        warn("[Teleport] ‚ö†Ô∏è Target name is nil!")
        return
    end

    if targetName == "Sisyphus State" then
        targetName = "Sisyphus Statue"
    end

    -- CRITICAL: Block teleport to TELEPORT_LOCATION if AUTO_QUEST is running
    -- Only allow quest locations (Treasure Room, Sisyphus Statue, Ancient Jungle, Sacred Temple)
    local questLocations = {"Treasure Room", "Sisyphus Statue", "Ancient Jungle", "Sacred Temple"}
    local isQuestLocation = false
    for _, loc in ipairs(questLocations) do
        if targetName == loc then
            isQuestLocation = true
            break
        end
    end

    if autoQuestRunning and not isQuestLocation then
        warn(string.format("[Teleport] üö´ BLOCKED teleport to '%s' - AUTO_QUEST is active, only quest locations allowed!", targetName))
        print("[Teleport] ‚úÖ Auto Quest system will handle all teleports")
        return
    end

    local success, errorMsg = pcall(function()
        local character = player.Character or player.CharacterAdded:Wait()
        local rootPart = character and character:FindFirstChild("HumanoidRootPart")
        if not rootPart then
            warn("[Teleport] ‚ö†Ô∏è No HumanoidRootPart found!")
            return
        end

        local found = false
        for _, location in ipairs(teleportLocations) do
            if location.Name == targetName and location.CFrame then
                rootPart.CFrame = location.CFrame
                found = true
                print(string.format("[Teleport] ‚úÖ Teleported to: %s", targetName))
                break
            end
        end

        if not found then
            warn(string.format("[Teleport] ‚ö†Ô∏è Location '%s' not found in teleportLocations!", targetName))
        end
    end)

    if not success then
        warn("[Teleport] ‚ùå Error: " .. tostring(errorMsg))
    end
end

