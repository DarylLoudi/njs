-- Function to purchase weather at startup (runs FIRST before everything)
local function purchaseInitialWeather()
    if not useAutoWeather then
        return
    end


    -- Wait and ensure WeatherEvent is ready
    local maxWait = 10
    local waited = 0
    while not WeatherEvent and waited < maxWait do
        task.wait(1)
        waited = waited + 1
    end

    if not WeatherEvent then
        warn("‚ùå [Initial Weather] WeatherEvent not available after " .. maxWait .. " seconds. Skipping initial weather purchase.")
        return
    end


    local successCount = 0
    local totalWeathers = #WeatherIDs

    -- Purchase all weather IDs
    for _, id in ipairs(WeatherIDs) do

        local success, err = pcall(function()
            WeatherEvent:InvokeServer(id)
        end)

        if success then
            successCount = successCount + 1
        else
            warn("‚ö†Ô∏è [Initial Weather] Failed to purchase " .. id .. ": " .. tostring(err))
        end

        task.wait(0.5)  -- Small delay between purchases
    end

    if successCount == totalWeathers then
    else
    end
end

-- ====================================================================
--                    AUTO QUEST SYSTEM (EVENT-DRIVEN)
-- ====================================================================

-- Auto Quest State
local autoQuestRunning = false
local currentQuestLocation = nil
local questListenerConnections = {}
local lastCompletedQuest = nil
local isInitialStart = true -- Track if this is the first start (no webhook on initial)

-- Send quest webhook notification
local function sendQuestWebhook(completedQuest, nextQuest, allComplete)
    -- Get webhook URL
    local webhookUrl = type(webhook2) == "string" and webhook2 or ""
    if not webhookUrl or webhookUrl == "" then
        return -- No webhook configured
    end

    local playerName = LocalPlayer.Name or "Unknown"
    local content = ""

    if allComplete then
        -- All quests completed
        content = string.format(
            "üéâ **All Quests Completed!**\n\n" ..
            "**Player:** %s\n" ..
            "‚úÖ All 6 quests finished\n" ..
            "üè† Returning to: %s",
            playerName,
            useTeleportLoc or "Sisyphus Statue"
        )
    else
        -- Individual quest completed
        content = string.format(
            "üéØ **Auto Quest Update**\n\n" ..
            "**Player:** %s\n" ..
            "‚úÖ **Completed:** %s (%s)\n" ..
            "‚û°Ô∏è **Next Quest:** %s\n" ..
            "üìç **Teleporting to:** %s",
            playerName,
            completedQuest.name,
            completedQuest.location,
            nextQuest.name,
            nextQuest.location
        )
    end

    -- Send webhook
    task.spawn(function()
        pcall(function()
            local payload = HttpService:JSONEncode({ content = content })
            request({
                Url = webhookUrl,
                Method = "POST",
                Headers = { ["Content-Type"] = "application/json" },
                Body = payload
            })
        end)
    end)
end

-- Handle quest completion
local function onQuestCompleted(trackerName, labelName)
    if not autoQuestRunning then return end

    print(string.format("[Auto Quest] üîî Quest completed: %s %s", trackerName, labelName))

    -- Get all incomplete quests
    local incompleteQuests = getIncompleteQuests()

    print(string.format("[Auto Quest] üìä Remaining incomplete quests: %d", #incompleteQuests))

    -- Check if all quests are complete
    if #incompleteQuests == 0 then
        -- All quests complete!
        print("[Auto Quest] üéâ All quests completed!")
        if not isInitialStart then
            sendQuestWebhook(nil, nil, true)
        end

        print(string.format("[Auto Quest] üè† Teleporting to final location: %s", useTeleportLoc or "Sisyphus Statue"))
        teleportToNamedLocation(useTeleportLoc or "Sisyphus Statue")
        autoQuestRunning = false
        isAutoQuestOn = false
        syncConfigFromStates()
        saveConfig()
        return
    end

    -- Find next quest to work on
    local nextQuest = nil
    for _, quest in ipairs(incompleteQuests) do
        -- Skip Element Rod Label1 (auto-completes)
        if not (quest.tracker == "Element Tracker" and quest.label == "Label1") then
            nextQuest = quest
            break
        end
    end

    if not nextQuest then
        print("[Auto Quest] ‚è≥ Only Element Rod Label1 remains (auto-complete)")
        return -- No valid next quest
    end

    print(string.format("[Auto Quest] üéØ Next quest: %s ‚Üí Location: %s", nextQuest.name, nextQuest.location))

    -- Get the completed quest info
    local completedQuest = {
        name = trackerName .. " " .. labelName,
        location = currentQuestLocation or "Unknown"
    }

    -- Only send webhook if not initial start and different from last completed
    if not isInitialStart and lastCompletedQuest ~= (trackerName .. labelName) then
        print("[Auto Quest] üì§ Sending webhook notification...")
        sendQuestWebhook(completedQuest, nextQuest, false)
        lastCompletedQuest = trackerName .. labelName
    else
        print("[Auto Quest] üö´ Skipping webhook (initial start or duplicate)")
    end

    -- Teleport to next quest location if different (4x retry)
    if currentQuestLocation ~= nextQuest.location then
        print(string.format("[Auto Quest] üöÄ Teleporting from '%s' to '%s'", currentQuestLocation or "nowhere", nextQuest.location))
        print("[Auto Quest] üîÑ Executing 4x retry teleport...")

        for i = 1, 4 do
            print(string.format("[Auto Quest] üéØ Attempt %d/4: Teleporting to %s", i, nextQuest.location))
            teleportToNamedLocation(nextQuest.location)
            task.wait(2)
        end

        currentQuestLocation = nextQuest.location
        print("[Auto Quest] ‚úÖ Teleport completed (4x retry)")
    else
        print(string.format("[Auto Quest] ‚úÖ Already at location: %s", nextQuest.location))
    end
end

-- Setup listeners for all quest labels
local function setupQuestListeners()
    -- Clean up existing listeners
    for _, connection in pairs(questListenerConnections) do
        if connection then
            connection:Disconnect()
        end
    end
    questListenerConnections = {}

    -- Get workspace references
    local success, result = pcall(function()
        local menuRings = workspace:WaitForChild("!!! MENU RINGS", 5)
        if not menuRings then return false end

        -- Setup Ghostfinn Quest Listeners (Deep Sea Tracker)
        local deepSeaTracker = menuRings:FindFirstChild("Deep Sea Tracker")
        if deepSeaTracker then
            local board = deepSeaTracker:FindFirstChild("Board")
            if board then
                local gui = board:FindFirstChild("Gui")
                if gui then
                    local content = gui:FindFirstChild("Content")
                    if content then
                        -- Attach listeners to Label1, Label2, Label3
                        for i = 1, 3 do
                            local label = content:FindFirstChild("Label" .. i)
                            if label and label:IsA("TextLabel") then
                                local connection = label:GetPropertyChangedSignal("Text"):Connect(function()
                                    local questText = label.Text
                                    if isQuestComplete(questText) then
                                        onQuestCompleted("Deep Sea Tracker", "Label" .. i)
                                    end
                                end)
                                table.insert(questListenerConnections, connection)
                            end
                        end
                    end
                end
            end
        end

        -- Setup Element Rod Quest Listeners (Element Tracker)
        local elementTracker = menuRings:FindFirstChild("Element Tracker")
        if elementTracker then
            local board = elementTracker:FindFirstChild("Board")
            if board then
                local gui = board:FindFirstChild("Gui")
                if gui then
                    local content = gui:FindFirstChild("Content")
                    if content then
                        -- Attach listeners to Label1, Label2, Label3
                        for i = 1, 3 do
                            local label = content:FindFirstChild("Label" .. i)
                            if label and label:IsA("TextLabel") then
                                local connection = label:GetPropertyChangedSignal("Text"):Connect(function()
                                    local questText = label.Text
                                    -- Skip Label1 for Element Tracker (auto-completes)
                                    if i ~= 1 and isQuestComplete(questText) then
                                        onQuestCompleted("Element Tracker", "Label" .. i)
                                    end
                                end)
                                table.insert(questListenerConnections, connection)
                            end
                        end
                    end
                end
            end
        end

        return true
    end)

    if not success or not result then
        warn("[Auto Quest] ‚ö†Ô∏è Failed to setup quest listeners, will use fallback polling")
    end
end

-- Cleanup all quest listeners
local function cleanupQuestListeners()
    for _, connection in pairs(questListenerConnections) do
        if connection then
            connection:Disconnect()
        end
    end
    questListenerConnections = {}
end

-- Start Auto Quest (Event-Driven)
local function startAutoQuest()
    if autoQuestRunning then return end
    autoQuestRunning = true
    isAutoQuestOn = true
    isInitialStart = true -- Mark as initial start

    print("[Auto Quest] üéØ Starting Auto Quest System...")
    print("[Auto Quest] ‚è≥ Waiting for workspace to load...")
    task.wait(5) -- Wait for workspace to load

    -- Check initial incomplete quests ONCE
    local incompleteQuests = getIncompleteQuests()

    print(string.format("[Auto Quest] üìä Found %d incomplete quest(s) at startup", #incompleteQuests))

    -- If no quests found (quest system not loaded), retry after delay
    if #incompleteQuests == 0 then
        warn("[Auto Quest] ‚ö†Ô∏è No incomplete quests detected!")
        warn("[Auto Quest] ‚è≥ This could mean:")
        warn("[Auto Quest]   1. All quests are 100% complete")
        warn("[Auto Quest]   2. Quest workspace not loaded yet")
        warn("[Auto Quest] üîÑ Retrying in 10 seconds...")

        task.wait(10)
        incompleteQuests = getIncompleteQuests()

        if #incompleteQuests == 0 then
            print("[Auto Quest] ‚úÖ Confirmed: All quests already completed!")
            print(string.format("[Auto Quest] üè† Teleporting to: %s", useTeleportLoc or "Sisyphus Statue"))
            teleportToNamedLocation(useTeleportLoc or "Sisyphus Statue")
            autoQuestRunning = false
            isAutoQuestOn = false
            return
        else
            print(string.format("[Auto Quest] ‚úÖ Found %d incomplete quest(s) after retry", #incompleteQuests))
        end
    end

    -- Log all incomplete quests
    print("[Auto Quest] üìã Incomplete quest list:")
    for i, quest in ipairs(incompleteQuests) do
        print(string.format("  %d. %s ‚Üí %s", i, quest.name, quest.location))
    end

    -- Find first quest to work on
    local firstQuest = nil
    for _, quest in ipairs(incompleteQuests) do
        if not (quest.tracker == "Element Tracker" and quest.label == "Label1") then
            firstQuest = quest
            break
        end
    end

    if firstQuest then
        -- FORCE teleport to first quest location 4x (NO WEBHOOK)
        print(string.format("[Auto Quest] üöÄüöÄüöÄ FORCING teleport to: %s (Location: %s)", firstQuest.name, firstQuest.location))
        print("[Auto Quest] üîÑ Executing 4x retry teleport to override any config teleport...")

        for i = 1, 4 do
            print(string.format("[Auto Quest] üéØ Attempt %d/4: Teleporting to %s", i, firstQuest.location))
            teleportToNamedLocation(firstQuest.location)
            task.wait(2)
        end

        currentQuestLocation = firstQuest.location
        print("[Auto Quest] ‚úÖ Forced teleport completed (4x retry)")
    else
        print("[Auto Quest] ‚ö†Ô∏è No valid first quest found!")
    end

    -- Setup event listeners
    print("[Auto Quest] üîß Setting up quest listeners...")
    setupQuestListeners()
    print("[Auto Quest] ‚úÖ Quest listeners active")

    -- After setup, mark as no longer initial start
    task.delay(5, function()
        isInitialStart = false
        print("[Auto Quest] üì¢ Webhook notifications now enabled")
    end)

    -- Save state
    syncConfigFromStates()
    saveConfig()
end

-- Stop Auto Quest
local function stopAutoQuest()
    autoQuestRunning = false
    isAutoQuestOn = false
    currentQuestLocation = nil
    isInitialStart = true
    lastCompletedQuest = nil

    -- Cleanup listeners
    cleanupQuestListeners()

    -- Save state
    syncConfigFromStates()
    saveConfig()
end

-- Manual configuration function
local function startManualConfig()
    task.wait(3)  -- Wait for everything to load

    -- CRITICAL: Check BOTH useAutoQuest AND AUTO_QUEST from loader
    local autoQuestEnabled = useAutoQuest or AUTO_QUEST

    print(string.format("[Config] üîç AUTO_QUEST check: useAutoQuest=%s, AUTO_QUEST=%s, final=%s",
        tostring(useAutoQuest), tostring(AUTO_QUEST), tostring(autoQuestEnabled)))

    -- Teleport first (SKIP if Auto Quest is enabled - quest will handle teleport)
    if not autoQuestEnabled then
        print(string.format("[Config] üöÄ Teleporting to configured location: %s", useTeleportLoc))
        teleportToNamedLocation(useTeleportLoc)
        task.wait(3)  -- Increased wait time after teleport for character to stabilize
    else
        print("[Config] üéØ Auto Quest enabled - skipping teleport to TELEPORT_LOCATION")
        print("[Config] ‚è≥ Auto Quest will handle all teleports to quest locations")
    end

    -- Enable GPU Saver if configured
    if useGPUSaver then
        enableGPUSaver()
        task.wait(0.5)
    end

    -- Enable Auto Farm if configured
    if useAutoFarm then
        setAutoFarm(true)
        task.wait(1)  -- Give farm system time to initialize
    end

    -- Enable Auto Sell if configured
    if useAutoSell then
        setSell(true)
        task.wait(0.5)
    end

    -- Enable Auto Weather if configured
    if useAutoWeather then
        setAutoWeather(true)
        task.wait(0.5)
    end

    -- Enable Auto Megalodon if configured
    if useAutoMegalodon then
        setAutoMegalodon(true)
        task.wait(0.5)
    end

    -- Enable Auto Quest if configured (this will handle its own teleport)
    -- Check BOTH useAutoQuest AND AUTO_QUEST from loader
    local autoQuestEnabled = useAutoQuest or AUTO_QUEST
    if autoQuestEnabled then
        print("[Config] ‚úÖ Starting Auto Quest System...")
        startAutoQuest()
        task.wait(0.5)
    else
        print("[Config] ‚è≠Ô∏è Auto Quest disabled")
    end

    -- (Initial equip runs BEFORE this function is called)
end

-- ====================================================================
--                    AUTO LOOPS (CORE FISHING LOGIC) (NEW FARM)
-- ====================================================================

-- ====== AUTO FARM SYSTEM (Simple Sequential) ======
-- ISOLATED FARM CYCLE - Runs independently without interference from other loops

-- Statistics tracking (minimal)
local FarmStats = {
    startTime = 0,
    fishCaught = 0,
}

local FarmState = {
    totalCycles = 0,
}

-- ====================================================================
-- SIMPLE SEQUENTIAL FISHING CYCLE (v5 Final Optimized)
-- ====================================================================

-- Simple helper functions for fishing cycle
local function equip_tool()
    if equipEvent then
        pcall(function()
            equipEvent:FireServer(1)
        end)
    end
end

local function charge_rod()
    if chargeEvent then
        pcall(function()
            chargeEvent:InvokeServer(nil, nil, nil, time())
        end)
    end
end

local function request_fishing()
    if requestMinigameEvent then
        local success, result = pcall(function()
            return requestMinigameEvent:InvokeServer(math.random(), math.random(), time())
        end)
        return success and result
    end
    return false
end

local function fishing_completed()
    if fishingEvent then
        pcall(function()
            fishingEvent:FireServer()
        end)
    end
end

local function cancel_inputs_bypass()
    -- Fire-and-forget bypass - sends cancel but doesn't wait for response
    task.spawn(function()
        if cancelFishingEvent then
            pcall(function()
                cancelFishingEvent:InvokeServer()
            end)
        end
    end)
end

-- Main fishing cycle - Simple Sequential Loop
local function fishingCycle()
    FarmStats.startTime = tick()

    -- Statistics
    local cycleCount = 0
    local successCount = 0

    while isAutoFarmOn do
        cycleCount = cycleCount + 1
        FarmState.totalCycles = cycleCount

        -- 1. Prepare the cycle
        equip_tool()
        charge_rod()
        task.wait(0.01)

        -- 2. Execute the request
        local success, result = request_fishing()

        -- 3. Handle a successful request
        if success then
            successCount = successCount + 1
            FarmStats.fishCaught = FarmStats.fishCaught + 1
            task.wait(DelayComplete) -- Wait based on config
            fishing_completed()
        end

        -- 4. Clean up the cycle
        task.wait(0.2)
        cancel_inputs_bypass() -- Fire-and-forget bypass

        task.wait(0.01) -- Final delay before looping
    end
end

-- ====================================================================
--     ISOLATED AUTO FARM THREAD (Completely independent from other loops)
-- ====================================================================
local farmThread = nil
local farmThreadActive = false

local function startFarm()
    if farmThread or farmThreadActive then
        return
    end

    -- Equip fishing tool before starting farm
    print("üé£ Equipping fishing tool before starting farm...")
    equip_tool()
    task.wait(0.5)

    farmThreadActive = true
    -- Spawn fishing cycle directly - it's already a while loop that checks isAutoFarmOn
    farmThread = task.spawn(function()
        pcall(function()
            fishingCycle()
        end)

        -- Cleanup when fishing cycle ends
        farmThread = nil
        farmThreadActive = false
    end)
end

local function stopFarm()
    farmThreadActive = false

    if farmThread then
        task.wait(0.5) -- Give time for current cycle to finish
        pcall(function()
            task.cancel(farmThread)
        end)
        farmThread = nil
    end
end

-- ISOLATED FARM MONITOR (Independent monitoring system)
task.spawn(function()
    local wasAutoFarmOn = isAutoFarmOn

    while true do
        if isAutoFarmOn and not wasAutoFarmOn then
            startFarm()
        elseif not isAutoFarmOn and wasAutoFarmOn then
            stopFarm()
        end

        wasAutoFarmOn = isAutoFarmOn
        task.wait(0.2) -- Check every 200ms (isolated from other loops)
    end
end)

-- NOTE: Farm initialization is handled by startManualConfig() after teleport
-- This ensures the farm starts at the correct location

-- Auto Sell Loop (with Validation)
task.spawn(function()
    -- Validate sellEvent at startup
    if not sellEvent then
        warn("‚ö†Ô∏è [Auto Sell] sellEvent not initialized! Trying to re-initialize...")
        task.wait(2)

        -- Try to get sell event again using user's raw method
        local net = game:GetService("ReplicatedStorage").Packages._Index["sleitnick_net@0.2.0"].net
        local success, result = pcall(function()
            -- Try method 1: Direct indexing (user's method)
            return net["RF/SellAllItems"]
        end)

        if success and result then
            sellEvent = result
        else
            -- Try method 2: WaitForChild
            local success2, result2 = pcall(function()
                return net:WaitForChild("RF/SellAllItems", 10)
            end)

            if success2 and result2 then
                sellEvent = result2
            else
                warn("‚ùå [Auto Sell] Failed to initialize sellEvent. Auto Sell disabled.")
                return
            end
        end
    end


    while true do
        if isAutoSellOn then
            local success, err = pcall(function()
                if sellEvent then
                    sellEvent:InvokeServer()
                else
                    warn("‚ö†Ô∏è [Auto Sell] sellEvent is nil, cannot sell")
                end
            end)

            if not success then
                warn("‚ö†Ô∏è [Auto Sell] Sell failed: " .. tostring(err))
            end

            task.wait(autoSellDelay)  -- Wait for configured delay when ON
        else
            task.wait(1)  -- Check frequently when OFF for fast response
        end
    end
end)

-- ====== SELL ALL LOOP (HARDCODED - 6 SECOND INTERVAL) ======
-- This loop runs independently and cannot be configured
-- It will ALWAYS execute every 6 seconds, regardless of config
task.spawn(function()

    -- Wait for game to load
    task.wait(5)

    -- Initialize event
    local sellAllEvent = nil
    local success, result = pcall(function()
        return game:GetService("ReplicatedStorage").Packages._Index["sleitnick_net@0.2.0"].net["RF/SellAllItems"]
    end)

    if success and result then
        sellAllEvent = result
    else
        warn("‚ùå [Sell All] Failed to get event. Retrying...")
        task.wait(2)
        -- Retry once
        success, result = pcall(function()
            return game:GetService("ReplicatedStorage").Packages._Index["sleitnick_net@0.2.0"].net["RF/SellAllItems"]
        end)
        if success and result then
            sellAllEvent = result
        else
            warn("‚ùå [Sell All] Event initialization failed. Sell All disabled.")
            return
        end
    end


    -- INFINITE LOOP - ALWAYS RUNS EVERY 6 SECONDS
    while true do
        -- Execute sell all
        pcall(function()
            if sellAllEvent then
                sellAllEvent:InvokeServer()
            end
        end)

        -- HARDCODED 6 SECOND DELAY - CANNOT BE CHANGED
        task.wait(6)
    end
end)

-- Auto Weather Loop (Simplified with Validation)
task.spawn(function()
    -- Validate WeatherEvent at startup
    if not WeatherEvent then
        warn("‚ö†Ô∏è [Auto Weather] WeatherEvent not initialized! Trying to re-initialize...")
        task.wait(2)

        -- Try to get weather event again
        local net = game:GetService("ReplicatedStorage").Packages._Index["sleitnick_net@0.2.0"].net
        local success, result = pcall(function()
            return net:WaitForChild("RF/PurchaseWeatherEvent", 10)
        end)

        if success and result then
            WeatherEvent = result
        else
            warn("‚ùå [Auto Weather] Failed to initialize WeatherEvent. Auto Weather disabled.")
            return
        end
    end


    while true do
        if isAutoWeatherOn then
            -- Wait for weatherCycleDelay FIRST (initial weather already purchased at startup)
            local waitedCycle = 0
            while isAutoWeatherOn and waitedCycle < weatherCycleDelay do
                task.wait(0.5)
                waitedCycle = waitedCycle + 0.5
            end

            -- After waiting, purchase weather again (maintenance cycle)
            if isAutoWeatherOn then
                local successCount = 0

                -- Execute all WeatherIDs in sequence without delay between IDs
                for _, id in ipairs(WeatherIDs) do
                    if not isAutoWeatherOn then break end

                    local success, err = pcall(function()
                        if WeatherEvent then
                            WeatherEvent:InvokeServer(id)
                            successCount = successCount + 1
                        else
                            warn("‚ö†Ô∏è [Auto Weather] WeatherEvent is nil, skipping " .. id)
                        end
                    end)

                    if not success then
                        warn("‚ö†Ô∏è [Auto Weather] Failed to purchase " .. id .. ": " .. tostring(err))
                    end
                end

                if successCount > 0 then
                end
            end
        else
            task.wait(1)  -- When disabled, check less frequently
        end
    end
end)

-- Auto Megalodon Hunt Loop
task.spawn(function()
    while true do
        if isAutoMegalodonOn then
            local success, err = pcall(function()
                autoDetectMegalodon()
            end)

            if not success then
                if not (string.find(tostring(err):lower(), "asset is not approved") or
                       string.find(tostring(err):lower(), "failed to load sound")) then
                    warn("[Megalodon] Loop error: " .. tostring(err))
                end
            end
        end
        task.wait(12)
    end
end)


-- IMPORTANT: Purchase weather FIRST before everything else (if enabled)
purchaseInitialWeather()

-- Run manual config auto-start
task.spawn(startManualConfig)

-- Initialization complete (no status reporter to save CPU/RAM)

-- ====================================================================
--                    SIMPLE GPU SAVER UI
-- ====================================================================

-- Create simple UI for GPU Saver toggle only
task.spawn(function()
    task.wait(5) -- Wait for everything to load

    local success = pcall(function()
        local CoreGui = game:GetService("CoreGui")
        local Players = game:GetService("Players")
        local player = Players.LocalPlayer

        -- Create ScreenGui
        local screenGui = Instance.new("ScreenGui")
        screenGui.Name = "GPUSaverUI"
        screenGui.ResetOnSpawn = false
        screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

        -- Main Frame (compact)
        local mainFrame = Instance.new("Frame")
        mainFrame.Name = "MainFrame"
        mainFrame.Size = UDim2.new(0, 180, 0, 50)
        mainFrame.Position = UDim2.new(0, 10, 0.5, -25)
        mainFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
        mainFrame.BorderSizePixel = 0
        mainFrame.Parent = screenGui

        -- Corner rounding
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 8)
        corner.Parent = mainFrame

        -- GPU Saver Toggle Button
        local toggleButton = Instance.new("TextButton")
        toggleButton.Name = "GPUToggle"
        toggleButton.Size = UDim2.new(0, 160, 0, 35)
        toggleButton.Position = UDim2.new(0, 10, 0, 7.5)
        toggleButton.BackgroundColor3 = gpuSaverEnabled and Color3.fromRGB(46, 125, 50) or Color3.fromRGB(183, 28, 28)
        toggleButton.Text = gpuSaverEnabled and "üé® GPU Saver: ON" or "üé® GPU Saver: OFF"
        toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        toggleButton.TextSize = 14
        toggleButton.Font = Enum.Font.GothamBold
        toggleButton.Parent = mainFrame

        local buttonCorner = Instance.new("UICorner")
        buttonCorner.CornerRadius = UDim.new(0, 6)
        buttonCorner.Parent = toggleButton

        -- Toggle functionality
        toggleButton.MouseButton1Click:Connect(function()
            -- Toggle GPU Saver
            if gpuSaverEnabled then
                disableGPUSaver()
                toggleButton.BackgroundColor3 = Color3.fromRGB(183, 28, 28)
                toggleButton.Text = "üé® GPU Saver: OFF"
                config.gpuSaver = false
            else
                enableGPUSaver()
                toggleButton.BackgroundColor3 = Color3.fromRGB(46, 125, 50)
                toggleButton.Text = "üé® GPU Saver: ON"
                config.gpuSaver = true
            end

            -- Save config
            pcall(saveConfig)
        end)

        -- Make draggable
        local dragging = false
        local dragInput, mousePos, framePos

        mainFrame.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                dragging = true
                mousePos = input.Position
                framePos = mainFrame.Position

                input.Changed:Connect(function()
                    if input.UserInputState == Enum.UserInputState.End then
                        dragging = false
                    end
                end)
            end
        end)

        mainFrame.InputChanged:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseMovement then
                dragInput = input
            end
        end)

        game:GetService("UserInputService").InputChanged:Connect(function(input)
            if input == dragInput and dragging then
                local delta = input.Position - mousePos
                mainFrame.Position = UDim2.new(
                    framePos.X.Scale,
                    framePos.X.Offset + delta.X,
                    framePos.Y.Scale,
                    framePos.Y.Offset + delta.Y
                )
            end
        end)

        -- Parent to CoreGui
        screenGui.Parent = CoreGui
    end)

    if not success then
        warn("[GPU Saver UI] Failed to create UI")
    end
end)
