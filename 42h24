        questLabels[i] = questLabel
    end

    -- Update quest labels with ALL incomplete quests (for display only)
    local function updateQuestLabels()
        local incompleteQuests = getAllIncompleteQuestsForDisplay()

        -- Update each label
        for i = 1, 6 do
            if incompleteQuests[i] then
                questLabels[i].Text = "üèÜ " .. incompleteQuests[i].name .. ": " .. incompleteQuests[i].text
                questLabels[i].Visible = true
            else
                questLabels[i].Visible = false
            end
        end
    end

