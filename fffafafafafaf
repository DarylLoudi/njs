print("üéÆ Ready to fish! Configuration is controlled via config_loader.lua")
print(string.rep("=", 80))

-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
-- AUTO SAVE CONFIG (jika enabled di config_loader.lua)
-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê

local saveConfigEnabled = config["Save Config"] and config["Save Config"]["Enabled"] or false

if saveConfigEnabled then
    print("\nüíæ Save Config ENABLED - Current settings will be saved to file")

    task.spawn(function()
        task.wait(5)  -- Wait 5 seconds after everything is loaded

        print("\n" .. string.rep("=", 80))
        print("üíæ AUTO-SAVING CURRENT CONFIGURATION...")
        print(string.rep("=", 80))

        local saveSuccess = saveConfig()

        if saveSuccess then
            print("‚úÖ Configuration saved successfully!")
            print("üìÅ File location: " .. getConfigFilePath())
            print("üîÑ Settings will be auto-loaded next time if 'Load Config' is enabled")
        else
            warn("‚ùå Failed to save configuration")
        end

        print(string.rep("=", 80) .. "\n")
    end)
else
    print("\nüíæ Save Config DISABLED - Settings will NOT be saved to file\n")
end
