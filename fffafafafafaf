-- Function to disable megalodon lock
local function disableMegalodonLock()
    megalodonLockActive = false
    megalodonLockedCFrame = nil
    megalodonPositionLocked = false
    megalodonCurrentEventPos = nil

    if megalodonLockConnection then
        megalodonLockConnection:Disconnect()
        megalodonLockConnection = nil
    end

    if megalodonBodyVelocity then
        megalodonBodyVelocity:Destroy()
        megalodonBodyVelocity = nil
    end

    if megalodonBodyGyro then
        megalodonBodyGyro:Destroy()
        megalodonBodyGyro = nil
    end
end

-- Function to enable megalodon lock at current position (for Ocean teleport)
local function enableMegalodonLock()
    local char = player.Character
    if not char then return false end

    local root = char:FindFirstChild("HumanoidRootPart")
    if not root then return false end

    -- Disable any existing lock first
    disableMegalodonLock()

    task.wait(0.2)

    -- Set locked position at current location
    megalodonLockedCFrame = root.CFrame
    megalodonPositionLocked = true
    megalodonLockActive = true

    -- Create BodyVelocity to prevent falling
    megalodonBodyVelocity = Instance.new("BodyVelocity")
    megalodonBodyVelocity.Velocity = Vector3.new(0, 0, 0)
    megalodonBodyVelocity.MaxForce = Vector3.new(100000, 100000, 100000)
    megalodonBodyVelocity.P = 10000
    megalodonBodyVelocity.Parent = root

    -- Create BodyGyro to maintain orientation
    megalodonBodyGyro = Instance.new("BodyGyro")
    megalodonBodyGyro.CFrame = megalodonLockedCFrame
    megalodonBodyGyro.MaxTorque = Vector3.new(100000, 100000, 100000)
    megalodonBodyGyro.P = 10000
    megalodonBodyGyro.Parent = root

    -- Create Heartbeat connection to maintain position
    megalodonLockConnection = RunService.Heartbeat:Connect(function()
        if not root or not root.Parent or not megalodonLockActive then
            if megalodonLockConnection then
                megalodonLockConnection:Disconnect()
                megalodonLockConnection = nil
            end
            return
        end

        -- If player moves too far from locked position, teleport back
        if (root.Position - megalodonLockedCFrame.Position).Magnitude > 15 then
            root.CFrame = megalodonLockedCFrame
            if megalodonBodyVelocity then
                megalodonBodyVelocity.Velocity = Vector3.new(0, 0, 0)
            end
        end
    end)

    print("[Megalodon Lock] ‚úÖ Position locked at Ocean!")
    return true
end

-- Teleport to location
local function teleportToLocation(locationName)
    local cframeData = TELEPORT_LOCATIONS[locationName]

    if not cframeData then
        warn(string.format("[Teleport] ‚ùå Location '%s' not found!", locationName))
        return false
    end

    local success = pcall(function()
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:FindFirstChild("HumanoidRootPart")
        if hrp then
            hrp.CFrame = cframeData
            print(string.format("[Teleport] ‚úÖ Teleported to: %s", locationName))

            -- Auto-enable megalodon lock for Ocean location
            if locationName == "Ocean" then
                task.spawn(function()
                    task.wait(0.5) -- Wait for teleport to complete
                    enableMegalodonLock()
                    print("[Teleport] üîí Ocean position locked - you will stay above water!")
                end)
            end
        else
            warn("[Teleport] ‚ùå HumanoidRootPart not found!")
        end
    end)

    if not success then
        warn(string.format("[Teleport] ‚ùå Failed to teleport to: %s", locationName))
        return false
    end

    return true
end

-- Rapid teleport 3x to ensure arrival at location
local function rapidTeleport3x(locationName)
    print(string.format("\n[Teleport] üåê Rapid teleporting to: %s (3x attempts)", locationName))

    local successCount = 0
    for i = 1, 3 do
        print(string.format("[Teleport] Attempt %d/3...", i))
        local success = teleportToLocation(locationName)
        if success then
            successCount = successCount + 1
        end
        task.wait(0.15) -- Small delay between attempts
    end

    print(string.format("[Teleport] ‚úÖ Rapid teleport complete (%d/3 successful)", successCount))
    return successCount > 0
end

print("‚úÖ Teleport module ready\n")

-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
-- AUTO MEGALODON MODULE
-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê

-- Function to teleport to megalodon and lock position
local function teleportToMegalodon(pos, isEvent)
    local char = player.Character
    if not char then return end

    local root = char:FindFirstChild("HumanoidRootPart")
    local hum = char:FindFirstChild("Humanoid")
    if not root or not hum then return end

    local tPos = pos
    if type(pos) == "userdata" and pos.X then
        tPos = pos + Vector3.new(0, 5, 0)
    elseif type(pos) == "userdata" and pos.Position then
        tPos = pos.Position + Vector3.new(0, 5, 0)
    end

    if isEvent then
        if megalodonCurrentEventPos and (tPos - megalodonCurrentEventPos).Magnitude < 5 then
            return
        end

        megalodonCurrentEventPos = tPos
        disableMegalodonLock()

        local finalPos = tPos + Vector3.new(0, 8, 0)
        root.CFrame = CFrame.new(finalPos)
        task.wait(0.2)

        -- V6.6: Use the unified enableMegalodonLock() function instead of duplicate code
        enableMegalodonLock()
    else
        root.CFrame = CFrame.new(tPos)
    end
end

-- Function to resume farming after megalodon event
local function resumeFarmingAfterMegalodon(previousAutoFarmState)
    task.spawn(function()
        task.wait(1)

        local farmLocation = "Sisyphus Statue"
        local success = teleportToLocation(farmLocation)
        if success then
            task.wait(2)
        end

        local shouldResume = previousAutoFarmState
        if shouldResume == nil then
            shouldResume = _G.AUTO_FARM_ENABLED
        end

        if shouldResume and not _G.AUTO_FARM_ENABLED then
            _G.AUTO_FARM_ENABLED = true
            if UIComponents.AutoFarmToggle then
                pcall(function()
                    UIComponents.AutoFarmToggle:SetValue(true)
                end)
            end
        end
    end)
end

-- Function to auto-detect megalodon event
local function autoDetectMegalodon()
    local eventFound = false
    local eventPosition = nil

    pcall(function()
        local menuRings = workspace:FindFirstChild("!!! MENU RINGS")
        if menuRings then
            for _, propsFolder in ipairs(menuRings:GetChildren()) do
                if propsFolder.Name == "Props" then
                    local huntFolder = propsFolder:FindFirstChild("Megalodon Hunt")
                    if huntFolder then
                        local colorPart = huntFolder:FindFirstChild("Color")
                        if colorPart and colorPart.Position then
                            eventPosition = colorPart.Position
                            eventFound = true
                            break
                        end
                    end
                end
            end
        end

        if not eventFound then
            for _, child in ipairs(workspace:GetChildren()) do
                if string.lower(child.Name) == "props" then
                    local megalodonHunt = child:FindFirstChild("Megalodon Hunt")
                    if megalodonHunt then
                        local colorPart = megalodonHunt:FindFirstChild("Color")
                        if colorPart and colorPart.Position then
                            eventPosition = colorPart.Position
                            eventFound = true
                            break
                        end
                    end
                end
            end
        end

        if not eventFound then
            for _, child in ipairs(workspace:GetChildren()) do
                for _, subChild in ipairs(child:GetChildren()) do
                    if string.find(string.lower(subChild.Name), "megalodon") then
                        local colorPart = subChild:FindFirstChild("Color")
                        if colorPart and colorPart.Position then
                            eventPosition = colorPart.Position
                            eventFound = true
                            break
                        end
                    end
                end
                if eventFound then break end
            end
        end
    end)

    if eventFound and eventPosition then
        if not megalodonEventActive then
            megalodonEventActive = true
            megalodonMissingAlertSent = false
            megalodonEventEndAlertSent = false
            megalodonPreEventFarmState = _G.AUTO_FARM_ENABLED
            megalodonEventStartedAt = os.time()
            print("[Megalodon] ü¶à Event detected! Teleporting...")
        end

        teleportToMegalodon(eventPosition, true)
    else
        local wasActive = megalodonEventActive
        if wasActive then
            megalodonEventActive = false
            disableMegalodonLock()

            if not megalodonEventEndAlertSent then
                megalodonEventEndAlertSent = true
                megalodonMissingAlertSent = true

                local eventEndedAt = os.time()
                local duration = eventEndedAt - megalodonEventStartedAt

                print(string.format("[Megalodon] üèÅ Event ended (Duration: %s)", FormatTime(duration)))
            end

            megalodonEventStartedAt = 0
            resumeFarmingAfterMegalodon(megalodonPreEventFarmState)
            megalodonPreEventFarmState = nil

        elseif not megalodonMissingAlertSent then
            megalodonMissingAlertSent = true
            print("[Megalodon] ‚ö†Ô∏è No event detected")
        end
    end
end

-- V6.6 OPTIMIZED: Megalodon Detection Loop Control
local megalodonDetectionTask = nil
local function startMegalodonDetection()
    if megalodonDetectionTask then return end
    megalodonDetectionTask = task.spawn(function()
        while _G.AUTO_MEGALODON_ENABLED do
            local success, err = pcall(function()
                autoDetectMegalodon()
            end)

            if not success then
                if not (string.find(tostring(err):lower(), "asset is not approved") or
                       string.find(tostring(err):lower(), "failed to load sound")) then
                    warn("[Megalodon] Loop error: " .. tostring(err))
                end
            end
            task.wait(12) -- Check every 12 seconds
        end
        megalodonDetectionTask = nil
    end)
end

local function stopMegalodonDetection()
    _G.AUTO_MEGALODON_ENABLED = false
    if megalodonDetectionTask then
        task.cancel(megalodonDetectionTask)
        megalodonDetectionTask = nil
    end
end

-- Main toggle function for auto megalodon
local function setAutoMegalodon(state)
    _G.AUTO_MEGALODON_ENABLED = state

    if not state then
        disableMegalodonLock()
        megalodonMissingAlertSent = false
        megalodonEventActive = false
        megalodonEventStartedAt = 0
        megalodonEventEndAlertSent = false
        megalodonPreEventFarmState = nil
        stopMegalodonDetection() -- V6.6: Stop detection loop
        print("[Megalodon] ‚èπÔ∏è Auto Megalodon stopped")
    else
        startMegalodonDetection() -- V6.6: Start detection loop
        print("[Megalodon] ‚ñ∂Ô∏è Auto Megalodon started")
    end
end

print("‚úÖ Auto Megalodon module ready\n")

-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
-- AUTO QUEST SYSTEM (EVENT-DRIVEN)
-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
--[[
    Auto Quest System - Event-driven quest tracking and completion

    Features:
    - Tracks 6 quest labels (3 Ghostfinn + 3 Element Rod)
    - Priority system: Ghostfinn first, then Element Rod
    - Event-driven using GetPropertyChangedSignal
    - Teleports to quest locations with 4x retry
    - Only displays incomplete quests
    - Webhook notifications on completion
]]

-- Get quest text from specific tracker and label (SILENT - no logging)
local function getQuestTextFromTracker(trackerName, labelName)
    local success, result = pcall(function()
        local menuRings = workspace:FindFirstChild("!!! MENU RINGS")
        if not menuRings then return "Quest not found" end

        local tracker = menuRings:FindFirstChild(trackerName)
        if not tracker then return "Quest not found" end

        local board = tracker:FindFirstChild("Board")
        if not board then return "Quest not found" end

        local gui = board:FindFirstChild("Gui")
        if not gui then return "Quest not found" end

        local content = gui:FindFirstChild("Content")
        if not content then return "Quest not found" end

        local label = content:FindFirstChild(labelName)
        if not label then return "Quest not found" end

        return label.Text or "No data"
    end)

    return success and result or "Error fetching quest"
end

-- Check if quest is 100% complete
local function isQuestComplete(questText)
    if not questText or questText == "" then return false end
    if questText:find("Quest not found") or questText:find("Error") then return false end

    -- Check for 100% completion patterns
    if questText:find("100%%") or questText:find("100 %%") then
        return true
    end

    -- Check for completion patterns like "Completed" or "Done"
    if questText:lower():find("completed") or questText:lower():find("done") then
        return true
    end

    return false
end

-- Get all Ghostfinn quests (Deep Sea Tracker)
local function getGhostfinnQuests()
    return {
        {tracker = "Deep Sea Tracker", label = "Label1", location = "Treasure Room"},
        {tracker = "Deep Sea Tracker", label = "Label2", location = "Sisyphus Statue"},
        {tracker = "Deep Sea Tracker", label = "Label3", location = "Sisyphus Statue"}
    }
end

-- Get all Element Rod quests (Element Tracker)
local function getElementRodQuests()
    return {
        {tracker = "Element Tracker", label = "Label1", location = "Auto", note = "Auto-completes with Ghostfinn"},
        {tracker = "Element Tracker", label = "Label2", location = "Ancient Jungle"},
        {tracker = "Element Tracker", label = "Label3", location = "Sacred Temple"}
    }
end

-- Get all incomplete quests
local function getIncompleteQuests()
    local incomplete = {}
    local hasIncompleteGhostfinn = false
    local hasValidGhostfinnData = false

    print("[Quest Check] üîç Checking all quests...")

    -- PRIORITY 1: Check Ghostfinn quests FIRST
    local ghostfinnQuests = getGhostfinnQuests()
    for _, quest in ipairs(ghostfinnQuests) do
        local questText = getQuestTextFromTracker(quest.tracker, quest.label)

        -- Check if quest data is valid (not error message)
        local isValidData = questText ~= "Quest not found" and questText ~= "Error fetching quest" and questText ~= "No data"

        if isValidData then
            hasValidGhostfinnData = true
            local isComplete = isQuestComplete(questText)
            print(string.format("[Quest Check] Ghostfinn %s: %s (Complete: %s)", quest.label, questText, tostring(isComplete)))

            if not isComplete then
                hasIncompleteGhostfinn = true
                table.insert(incomplete, {
                    name = "Ghostfinn " .. quest.label,
                    tracker = quest.tracker,
                    label = quest.label,
                    location = quest.location,
                    text = questText,
                    priority = 1  -- High priority
                })
            end
        else
            warn(string.format("[Quest Check] ‚ö†Ô∏è Ghostfinn %s: Invalid data '%s' - skipping", quest.label, questText))
        end
    end

    -- If no valid Ghostfinn data found, quest system not ready
    if not hasValidGhostfinnData then
        warn("[Quest Check] ‚ùå No valid Ghostfinn quest data found! Quest system may not be loaded yet.")
        return {} -- Return empty to prevent teleport
    end

    print(string.format("[Quest Check] üìä Ghostfinn status: %s", hasIncompleteGhostfinn and "INCOMPLETE" or "ALL COMPLETE"))

    -- PRIORITY 2: Only check Element Rod quests if ALL Ghostfinn quests are complete
    if not hasIncompleteGhostfinn then
        print("[Quest Check] ‚úÖ All Ghostfinn quests complete, checking Element Rod...")
        local elementQuests = getElementRodQuests()
        for _, quest in ipairs(elementQuests) do
            local questText = getQuestTextFromTracker(quest.tracker, quest.label)
            local isValidData = questText ~= "Quest not found" and questText ~= "Error fetching quest" and questText ~= "No data"

            if isValidData then
                local isComplete = isQuestComplete(questText)
                print(string.format("[Quest Check] Element Rod %s: %s (Complete: %s)", quest.label, questText, tostring(isComplete)))

                if not isComplete then
                    table.insert(incomplete, {
                        name = "Element Rod " .. quest.label,
                        tracker = quest.tracker,
                        label = quest.label,
                        location = quest.location,
                        text = questText,
                        note = quest.note,
                        priority = 2  -- Lower priority
                    })
                end
            else
                warn(string.format("[Quest Check] ‚ö†Ô∏è Element Rod %s: Invalid data '%s' - skipping", quest.label, questText))
            end
        end
    else
        print("[Quest Check] ‚è≠Ô∏è Skipping Element Rod quests (Ghostfinn not done yet)")
    end

    print(string.format("[Quest Check] üìã Total incomplete quests: %d", #incomplete))
    return incomplete
end

-- Get ALL incomplete quests for DISPLAY (no priority filter)
local function getAllIncompleteQuestsForDisplay()
    local incomplete = {}

    -- Check ALL Ghostfinn quests
    local ghostfinnQuests = getGhostfinnQuests()
    for _, quest in ipairs(ghostfinnQuests) do
        local questText = getQuestTextFromTracker(quest.tracker, quest.label)
        local isValidData = questText ~= "Quest not found" and questText ~= "Error fetching quest" and questText ~= "No data"

        if isValidData and not isQuestComplete(questText) then
            table.insert(incomplete, {
                name = "Ghostfinn " .. quest.label,
                tracker = quest.tracker,
                label = quest.label,
                location = quest.location,
                text = questText
            })
        end
    end

    -- Check ALL Element Rod quests (regardless of Ghostfinn status)
    local elementQuests = getElementRodQuests()
    for _, quest in ipairs(elementQuests) do
        local questText = getQuestTextFromTracker(quest.tracker, quest.label)
        local isValidData = questText ~= "Quest not found" and questText ~= "Error fetching quest" and questText ~= "No data"

        if isValidData and not isQuestComplete(questText) then
            table.insert(incomplete, {
                name = "Element Rod " .. quest.label,
                tracker = quest.tracker,
                label = quest.label,
                location = quest.location,
                text = questText,
                note = quest.note
            })
        end
    end

    return incomplete
end

-- Check if all quests are complete
local function areAllQuestsComplete()
    local incompleteQuests = getIncompleteQuests()
    return #incompleteQuests == 0
end

-- Teleport to named location (quest locations only)
local function teleportToQuestLocation(locationName)
    if not locationName then
        warn("[Auto Quest] ‚ö†Ô∏è Location name is nil!")
        return
    end

    -- Quest location mappings (from main script v2.lua)
    local questLocations = {
        ["Treasure Room"] = CFrame.new(3134.67529, 127.984032, -3257.58325),
        ["Sisyphus Statue"] = CFrame.new(-1318.41, 129.2, 1593.39),
        ["Ancient Jungle"] = CFrame.new(3116.5, 132.1, 2516.5),
        ["Sacred Temple"] = CFrame.new(6061, 130.8, 2024)
    }

    local targetCFrame = questLocations[locationName]
    if not targetCFrame then
        warn(string.format("[Auto Quest] ‚ö†Ô∏è Unknown location: %s", locationName))
        return
    end

    pcall(function()
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:FindFirstChild("HumanoidRootPart")
        if hrp then
            hrp.CFrame = targetCFrame
        end
    end)
end

-- Handle quest completion
local function onQuestCompleted(trackerName, labelName)
    if not autoQuestRunning then return end

    print(string.format("[Auto Quest] üîî Quest completed: %s %s", trackerName, labelName))

    -- Get all incomplete quests
    local incompleteQuests = getIncompleteQuests()

    print(string.format("[Auto Quest] üìä Remaining incomplete quests: %d", #incompleteQuests))

    -- Check if all quests are complete
    if #incompleteQuests == 0 then
        -- All quests complete!
        print("[Auto Quest] üéâ All quests completed!")
        autoQuestRunning = false
        _G.AUTO_QUEST_ENABLED = false
        return
    end

    -- Find next quest to work on
    local nextQuest = nil
    for _, quest in ipairs(incompleteQuests) do
        -- Skip Element Rod Label1 (auto-completes)
        if not (quest.tracker == "Element Tracker" and quest.label == "Label1") then
            nextQuest = quest
            break
        end
    end

    if not nextQuest then
        print("[Auto Quest] ‚è≥ Only Element Rod Label1 remains (auto-complete)")
        return -- No valid next quest
    end

    print(string.format("[Auto Quest] üéØ Next quest: %s ‚Üí Location: %s", nextQuest.name, nextQuest.location))

    -- Teleport to next quest location if different (4x retry)
    if currentQuestLocation ~= nextQuest.location then
        print(string.format("[Auto Quest] üöÄ Teleporting from '%s' to '%s'", currentQuestLocation or "nowhere", nextQuest.location))
        print("[Auto Quest] üîÑ Executing 4x retry teleport...")

        for i = 1, 4 do
            print(string.format("[Auto Quest] üéØ Attempt %d/4: Teleporting to %s", i, nextQuest.location))
            teleportToQuestLocation(nextQuest.location)
            task.wait(2)
        end

        currentQuestLocation = nextQuest.location
        print("[Auto Quest] ‚úÖ Teleport completed (4x retry)")
    else
        print(string.format("[Auto Quest] ‚úÖ Already at location: %s", nextQuest.location))
    end
end

-- Setup listeners for all quest labels
local function setupQuestListeners()
    -- Clean up existing listeners
    for _, connection in pairs(questListenerConnections) do
        if connection then
            connection:Disconnect()
        end
    end
    questListenerConnections = {}

    local success, result = pcall(function()
        local menuRings = workspace:FindFirstChild("!!! MENU RINGS")
        if not menuRings then
            warn("[Auto Quest] ‚ö†Ô∏è !!! MENU RINGS not found in workspace")
            return false
        end

        -- Setup listeners for Deep Sea Tracker (Ghostfinn)
        local deepSeaTracker = menuRings:FindFirstChild("Deep Sea Tracker")
        if deepSeaTracker then
            local board = deepSeaTracker:FindFirstChild("Board")
            if board then
                local gui = board:FindFirstChild("Gui")
                if gui then
                    local content = gui:FindFirstChild("Content")
                    if content then
                        for _, labelName in ipairs({"Label1", "Label2", "Label3"}) do
                            local label = content:FindFirstChild(labelName)
                            if label then
                                local connection = label:GetPropertyChangedSignal("Text"):Connect(function()
                                    local questText = label.Text
                                    if isQuestComplete(questText) then
                                        onQuestCompleted("Deep Sea Tracker", labelName)
                                    end
                                end)
                                table.insert(questListenerConnections, connection)
                                print(string.format("[Auto Quest] ‚úÖ Listener set: Deep Sea Tracker %s", labelName))
                            end
                        end
                    end
                end
            end
        end

        -- Setup listeners for Element Tracker (Element Rod)
        local elementTracker = menuRings:FindFirstChild("Element Tracker")
        if elementTracker then
            local board = elementTracker:FindFirstChild("Board")
            if board then
                local gui = board:FindFirstChild("Gui")
                if gui then
                    local content = gui:FindFirstChild("Content")
                    if content then
                        for _, labelName in ipairs({"Label1", "Label2", "Label3"}) do
                            local label = content:FindFirstChild(labelName)
                            if label then
                                local connection = label:GetPropertyChangedSignal("Text"):Connect(function()
                                    local questText = label.Text
                                    if isQuestComplete(questText) then
                                        onQuestCompleted("Element Tracker", labelName)
                                    end
                                end)
                                table.insert(questListenerConnections, connection)
                                print(string.format("[Auto Quest] ‚úÖ Listener set: Element Tracker %s", labelName))
                            end
                        end
                    end
                end
            end
        end

        return true
    end)

    if not success or not result then
        warn("[Auto Quest] ‚ö†Ô∏è Failed to setup quest listeners, will use fallback polling")
    end
end

-- Cleanup all quest listeners
local function cleanupQuestListeners()
    for _, connection in pairs(questListenerConnections) do
        if connection then
            connection:Disconnect()
        end
    end
    questListenerConnections = {}
end

-- Start Auto Quest (Event-Driven)
local function startAutoQuest()
    if autoQuestRunning then return end
    autoQuestRunning = true
    _G.AUTO_QUEST_ENABLED = true
    isInitialStart = true

    print("[Auto Quest] üéØ Starting Auto Quest System...")
    print("[Auto Quest] ‚è≥ Waiting for workspace to load...")
    task.wait(5)

    -- Check initial incomplete quests ONCE
    local incompleteQuests = getIncompleteQuests()

    print(string.format("[Auto Quest] üìä Found %d incomplete quest(s) at startup", #incompleteQuests))

    -- If no quests found (quest system not loaded), retry after delay
    if #incompleteQuests == 0 then
        warn("[Auto Quest] ‚ö†Ô∏è No incomplete quests detected!")
        warn("[Auto Quest] ‚è≥ This could mean:")
        warn("[Auto Quest]   1. All quests are 100% complete")
        warn("[Auto Quest]   2. Quest workspace not loaded yet")
        warn("[Auto Quest] üîÑ Retrying in 10 seconds...")

        task.wait(10)
        incompleteQuests = getIncompleteQuests()

        if #incompleteQuests == 0 then
            print("[Auto Quest] ‚úÖ Confirmed: All quests already completed!")
            autoQuestRunning = false
            _G.AUTO_QUEST_ENABLED = false
            return
        else
            print(string.format("[Auto Quest] ‚úÖ Found %d incomplete quest(s) after retry", #incompleteQuests))
        end
    end

    -- Log all incomplete quests
    print("[Auto Quest] üìã Incomplete quest list:")
    for i, quest in ipairs(incompleteQuests) do
        print(string.format("  %d. %s ‚Üí %s", i, quest.name, quest.location))
    end

    -- Find first quest to work on
    local firstQuest = nil
    for _, quest in ipairs(incompleteQuests) do
        if not (quest.tracker == "Element Tracker" and quest.label == "Label1") then
            firstQuest = quest
            break
        end
    end

    if firstQuest then
        -- FORCE teleport to first quest location 4x (NO WEBHOOK)
        print(string.format("[Auto Quest] üöÄüöÄüöÄ FORCING teleport to: %s (Location: %s)", firstQuest.name, firstQuest.location))
        print("[Auto Quest] üîÑ Executing 4x retry teleport to override any config teleport...")

        for i = 1, 4 do
            print(string.format("[Auto Quest] üéØ Attempt %d/4: Teleporting to %s", i, firstQuest.location))
            teleportToQuestLocation(firstQuest.location)
            task.wait(2)
        end

        currentQuestLocation = firstQuest.location
        print("[Auto Quest] ‚úÖ Forced teleport completed (4x retry)")
    else
        print("[Auto Quest] ‚ö†Ô∏è No valid first quest found!")
    end

    -- Setup event listeners
    print("[Auto Quest] üîß Setting up quest listeners...")
    setupQuestListeners()
    print("[Auto Quest] ‚úÖ Quest listeners active")

    -- After setup, mark as no longer initial start
    task.delay(5, function()
        isInitialStart = false
        print("[Auto Quest] üì¢ Quest system fully initialized")
    end)
end

-- Stop Auto Quest
local function stopAutoQuest()
    autoQuestRunning = false
    _G.AUTO_QUEST_ENABLED = false
    currentQuestLocation = nil
    isInitialStart = true
    lastCompletedQuest = nil

    -- Cleanup listeners
    cleanupQuestListeners()

    print("[Auto Quest] ‚èπÔ∏è Auto Quest stopped")
end

-- Main toggle function for auto quest
local function setAutoQuest(state)
    if state then
        startAutoQuest()
    else
        stopAutoQuest()
    end
end

print("‚úÖ Auto Quest module ready\n")
-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
-- SECTION 4: CONFIG LOADER & AUTO-START SYSTEM (HEADLESS MODE)
-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
--[[
    This section loads configuration from _G.AutoFishConfig (set by config_loader.lua)
    and automatically starts all enabled features based on the config.

    NO GUI - All settings are controlled via config_loader.lua
]]

print(string.rep("=", 80))
print("‚öôÔ∏è LOADING CONFIGURATION FROM _G.AutoFishConfig...")
print(string.rep("=", 80))

-- Check if config exists
if not _G.AutoFishConfig then
    error("‚ùå ERROR: _G.AutoFishConfig not found! Please load config_loader.lua first!")
    return
end

local config = _G.AutoFishConfig

-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
-- LOAD CONFIG: Check if Load Config is enabled
-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê

local loadConfigEnabled = config["Load Config"] and config["Load Config"]["Enabled"] or false

if loadConfigEnabled then
    print("\nüîÑ Load Config ENABLED - Attempting to load from saved file...")

    local loadedConfig = loadConfig()

    if loadedConfig then
        print("‚úÖ Successfully loaded config from file!")
        print("üìã Applying settings from saved config file...")
        print("‚ö†Ô∏è  Settings from config_loader.lua will be IGNORED\n")

        -- Override config with loaded settings
        if loadedConfig.AUTO_FARM_ENABLED ~= nil then
            config["Auto Farm"]["Auto Farm V1"]["Enabled"] = loadedConfig.AUTO_FARM_ENABLED
        end
        if loadedConfig.FARM_SUCCESS_DELAY then
            config["Auto Farm"]["Auto Farm V1"]["Success Delay"] = loadedConfig.FARM_SUCCESS_DELAY
        end
        if loadedConfig.AUTO_FARM_V2_ENABLED ~= nil then
            config["Auto Farm"]["Auto Farm V2"]["Enabled"] = loadedConfig.AUTO_FARM_V2_ENABLED
        end
        if loadedConfig.JUNG_V2_STATE then
            if loadedConfig.JUNG_V2_STATE.baseDelayReel then
                config["Auto Farm"]["Auto Farm V2"]["Base Delay Reel"] = loadedConfig.JUNG_V2_STATE.baseDelayReel
            end
            if loadedConfig.JUNG_V2_STATE.baseDelayComplete then
                config["Auto Farm"]["Auto Farm V2"]["Base Delay Complete"] = loadedConfig.JUNG_V2_STATE.baseDelayComplete
            end
        end
        if loadedConfig.AUTO_SELL_ENABLED ~= nil then
            config["Auto Sell"]["Enabled"] = loadedConfig.AUTO_SELL_ENABLED
        end
        if loadedConfig.AUTO_SELL_DELAY then
            config["Auto Sell"]["Delay"] = loadedConfig.AUTO_SELL_DELAY
        end
        if loadedConfig.AUTO_ARTIFACT_ENABLED ~= nil then
            config["Auto Artifact"]["Enabled"] = loadedConfig.AUTO_ARTIFACT_ENABLED
        end
        if loadedConfig.artifactSkipToggles then
            config["Auto Artifact"]["Skip Hourglass Diamond"] = loadedConfig.artifactSkipToggles[1] or false
            config["Auto Artifact"]["Skip Arrow Artifact"] = loadedConfig.artifactSkipToggles[2] or false
            config["Auto Artifact"]["Skip Diamond Artifact"] = loadedConfig.artifactSkipToggles[3] or false
            config["Auto Artifact"]["Skip Crescent Artifact"] = loadedConfig.artifactSkipToggles[4] or false
        end
        if loadedConfig.AUTO_WEATHER_ENABLED ~= nil then
            config["Auto Weather"]["Enabled"] = loadedConfig.AUTO_WEATHER_ENABLED
        end
        if loadedConfig.WEATHER_PURCHASE_DELAY then
            config["Auto Weather"]["Purchase Delay"] = loadedConfig.WEATHER_PURCHASE_DELAY
        end
        if loadedConfig.AUTO_TOTEM_ENABLED ~= nil then
            config["Auto Totem"]["Enabled"] = loadedConfig.AUTO_TOTEM_ENABLED
        end
        if loadedConfig.TOTEM_CYCLE_DELAY then
            config["Auto Totem"]["Cycle Delay"] = loadedConfig.TOTEM_CYCLE_DELAY
        end
        if loadedConfig.TOTEM_ITEM_ID then
            config["Auto Totem"]["Item ID"] = loadedConfig.TOTEM_ITEM_ID
        end
        if loadedConfig.AUTO_MEGALODON_ENABLED ~= nil then
            config["Auto Farm"]["Auto Megalodon"]["Enabled"] = loadedConfig.AUTO_MEGALODON_ENABLED
        end
        if loadedConfig.AUTO_QUEST_ENABLED ~= nil then
            config["Auto Farm"]["Auto Quest"]["Enabled"] = loadedConfig.AUTO_QUEST_ENABLED
        end
        if loadedConfig.AUTO_UPGRADE_ROD_ENABLED ~= nil then
            config["Auto Upgrade"]["Auto Upgrade Rod"] = loadedConfig.AUTO_UPGRADE_ROD_ENABLED
        end
        if loadedConfig.AUTO_UPGRADE_BAIT_ENABLED ~= nil then
            config["Auto Upgrade"]["Auto Upgrade Bait"] = loadedConfig.AUTO_UPGRADE_BAIT_ENABLED
        end
        if loadedConfig.FREEZE_CHAR_ENABLED ~= nil then
            config["Misc"]["Freeze Character"] = loadedConfig.FREEZE_CHAR_ENABLED
        end
        if loadedConfig.GPU_SAVER_ENABLED ~= nil then
            config["Misc"]["GPU Saver"] = loadedConfig.GPU_SAVER_ENABLED
        end
        if loadedConfig.DISABLE_ANIMATIONS_ENABLED ~= nil then
            config["Misc"]["Disable Animations"] = loadedConfig.DISABLE_ANIMATIONS_ENABLED
        end
        if loadedConfig.TELEPORT_ENABLED ~= nil then
            config["Teleport"]["Enabled"] = loadedConfig.TELEPORT_ENABLED
        end
        if loadedConfig.TELEPORT_LOCATION then
            config["Teleport"]["Location"] = loadedConfig.TELEPORT_LOCATION
        end
        if loadedConfig.WEBHOOK_ENABLED ~= nil then
            config["Webhook"]["Enabled"] = loadedConfig.WEBHOOK_ENABLED
        end
        if loadedConfig.WEBHOOK_SECRET then
            config["Webhook"]["URL"] = loadedConfig.WEBHOOK_SECRET
        end

        print("‚úÖ All settings from saved file have been applied!\n")
    else
        print("‚ö†Ô∏è  No saved config file found, using settings from config_loader.lua\n")
    end
else
    print("\nüìã Load Config DISABLED - Using settings from config_loader.lua\n")
end

-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
-- APPLY CONFIGURATION TO GLOBAL VARIABLES
-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê

print("\nüìã Applying configuration settings...\n")

-- Auto Farm V1 Settings
_G.AUTO_FARM_ENABLED = config["Auto Farm"]["Auto Farm V1"]["Enabled"]
_G.FARM_SUCCESS_DELAY = config["Auto Farm"]["Auto Farm V1"]["Success Delay"]
print(string.format("  üé£ Auto Farm V1: %s (Delay: %.1fs)",
    _G.AUTO_FARM_ENABLED and "‚úÖ ENABLED" or "‚ùå DISABLED", _G.FARM_SUCCESS_DELAY))

-- Auto Farm V2 (Jung Fast) Settings
_G.AUTO_FARM_V2_ENABLED = config["Auto Farm"]["Auto Farm V2"]["Enabled"]
_G.JUNG_V2_STATE.baseDelayReel = config["Auto Farm"]["Auto Farm V2"]["Base Delay Reel"]
_G.JUNG_V2_STATE.baseDelayComplete = config["Auto Farm"]["Auto Farm V2"]["Base Delay Complete"]
print(string.format("  ‚ö° Auto Farm V2 (Jung): %s (Reel: %.1fs, Complete: %.1fs)",
    _G.AUTO_FARM_V2_ENABLED and "‚úÖ ENABLED" or "‚ùå DISABLED",
    _G.JUNG_V2_STATE.baseDelayReel, _G.JUNG_V2_STATE.baseDelayComplete))

-- Auto Megalodon Settings
_G.AUTO_MEGALODON_ENABLED = config["Auto Farm"]["Auto Megalodon"]["Enabled"]
print(string.format("  ü¶à Auto Megalodon: %s",
    _G.AUTO_MEGALODON_ENABLED and "‚úÖ ENABLED" or "‚ùå DISABLED"))

-- Auto Quest Settings
_G.AUTO_QUEST_ENABLED = config["Auto Farm"]["Auto Quest"]["Enabled"]
print(string.format("  üìú Auto Quest: %s",
    _G.AUTO_QUEST_ENABLED and "‚úÖ ENABLED" or "‚ùå DISABLED"))

-- Auto Sell Settings
local autoSellConfigEnabled = config["Auto Sell"]["Enabled"]
_G.AUTO_SELL_DELAY = config["Auto Sell"]["Delay"]
print(string.format("  üí∞ Auto Sell: %s (Delay: %ds)",
    autoSellConfigEnabled and "‚úÖ ENABLED" or "‚ùå DISABLED", _G.AUTO_SELL_DELAY))

-- Auto Artifact Settings
_G.AUTO_ARTIFACT_ENABLED = config["Auto Artifact"]["Enabled"]
artifactSkipToggles[1] = config["Auto Artifact"]["Skip Hourglass Diamond"]
artifactSkipToggles[2] = config["Auto Artifact"]["Skip Arrow Artifact"]
artifactSkipToggles[3] = config["Auto Artifact"]["Skip Diamond Artifact"]
artifactSkipToggles[4] = config["Auto Artifact"]["Skip Crescent Artifact"]
print(string.format("  üóø Auto Artifact: %s",
    _G.AUTO_ARTIFACT_ENABLED and "‚úÖ ENABLED" or "‚ùå DISABLED"))
print(string.format("      Skip Temples: [T1:%s, T2:%s, T3:%s, T4:%s]",
    artifactSkipToggles[1] and "‚è≠Ô∏è" or "‚úÖ",
    artifactSkipToggles[2] and "‚è≠Ô∏è" or "‚úÖ",
    artifactSkipToggles[3] and "‚è≠Ô∏è" or "‚úÖ",
    artifactSkipToggles[4] and "‚è≠Ô∏è" or "‚úÖ"))

-- Auto Weather Settings
local autoWeatherConfigEnabled = config["Auto Weather"]["Enabled"]
_G.WEATHER_PURCHASE_DELAY = config["Auto Weather"]["Purchase Delay"]
print(string.format("  üå§Ô∏è Auto Weather: %s (Delay: %ds)",
    autoWeatherConfigEnabled and "‚úÖ ENABLED" or "‚ùå DISABLED", _G.WEATHER_PURCHASE_DELAY))

-- Auto Totem Settings
_G.AUTO_TOTEM_ENABLED = config["Auto Totem"]["Enabled"]
_G.TOTEM_CYCLE_DELAY = config["Auto Totem"]["Cycle Delay"]
_G.TOTEM_ITEM_ID = config["Auto Totem"]["Item ID"]
print(string.format("  üóø Auto Totem: %s (Cycle: %d min, Item ID: %d)",
    _G.AUTO_TOTEM_ENABLED and "‚úÖ ENABLED" or "‚ùå DISABLED",
    _G.TOTEM_CYCLE_DELAY, _G.TOTEM_ITEM_ID))

-- Auto Upgrade Settings
_G.AUTO_UPGRADE_ROD_ENABLED = config["Auto Upgrade"]["Auto Upgrade Rod"]
_G.AUTO_UPGRADE_BAIT_ENABLED = config["Auto Upgrade"]["Auto Upgrade Bait"]
print(string.format("  ‚¨ÜÔ∏è Auto Upgrade Rod: %s",
    _G.AUTO_UPGRADE_ROD_ENABLED and "‚úÖ ENABLED" or "‚ùå DISABLED"))
print(string.format("  ‚¨ÜÔ∏è Auto Upgrade Bait: %s",
    _G.AUTO_UPGRADE_BAIT_ENABLED and "‚úÖ ENABLED" or "‚ùå DISABLED"))

-- Misc Settings
_G.FREEZE_CHAR_ENABLED = config["Misc"]["Freeze Character"]
_G.GPU_SAVER_ENABLED = config["Misc"]["GPU Saver"]
_G.DISABLE_ANIMATIONS_ENABLED = config["Misc"]["Disable Animations"]
local removeIslands = config["Misc"]["Remove Islands"]
local unlockFPS = config["Misc"]["Unlock FPS"]

-- Sync local GPU Saver variable with global variable
gpuSaverEnabled = _G.GPU_SAVER_ENABLED
print(string.format("  üîí Freeze Character: %s",
    _G.FREEZE_CHAR_ENABLED and "‚úÖ ENABLED" or "‚ùå DISABLED"))
print(string.format("  üíæ GPU Saver: %s",
    _G.GPU_SAVER_ENABLED and "‚úÖ ENABLED" or "‚ùå DISABLED"))
print(string.format("  üé¨ Disable Animations: %s",
    _G.DISABLE_ANIMATIONS_ENABLED and "‚úÖ ENABLED" or "‚ùå DISABLED"))
print(string.format("  üèùÔ∏è Remove Islands: %s",
    removeIslands and "‚úÖ ENABLED" or "‚ùå DISABLED"))
print(string.format("  üìä Unlock FPS: %s",
    unlockFPS and "‚úÖ ENABLED" or "‚ùå DISABLED"))

-- Webhook Settings
_G.WEBHOOK_ENABLED = config["Webhook"]["Enabled"]
_G.WEBHOOK_SECRET = config["Webhook"]["URL"]
print(string.format("  üîî Webhook: %s",
    _G.WEBHOOK_ENABLED and "‚úÖ ENABLED" or "‚ùå DISABLED"))

-- Teleport Settings
_G.TELEPORT_ENABLED = config["Teleport"] and config["Teleport"]["Enabled"] or false
_G.TELEPORT_LOCATION = config["Teleport"] and config["Teleport"]["Location"] or "Ocean"
local teleportEnabled = _G.TELEPORT_ENABLED
local teleportLocation = _G.TELEPORT_LOCATION
print(string.format("  üåê Teleport: %s (Location: %s)",
    teleportEnabled and "‚úÖ ENABLED" or "‚ùå DISABLED", teleportLocation))

print("\n‚úÖ Configuration applied successfully!\n")

-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
-- AUTO-START ALL ENABLED FEATURES
-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê

print(string.rep("=", 80))
print("üöÄ AUTO-STARTING ENABLED FEATURES...")
print(string.rep("=", 80))
print("")

-- Wait for game to fully load
task.wait(2)

-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
-- STEP 0: TELEPORT TO LOCATION (EXECUTES FIRST BEFORE ALL OTHER FEATURES)
-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ

if teleportEnabled then
    print(string.format("\n  üåê Teleport ENABLED - Teleporting to: %s", teleportLocation))
    print("      This will execute 3x rapidly to ensure arrival at location...")
    task.wait(1)

    local teleportSuccess = rapidTeleport3x(teleportLocation)

    if teleportSuccess then
        print(string.format("  üåê ‚úÖ Teleport complete! You are now at: %s", teleportLocation))
        task.wait(2) -- Wait for teleport to stabilize before starting other features
        print("      Starting other features...\n")
    else
        warn("  üåê ‚ö†Ô∏è Teleport failed! Continuing with other features anyway...\n")
    end
else
    print("\n  üåê Teleport DISABLED - Staying at current location\n")
end

-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
-- STEP 1: START AUTO FARM (V1 or V2)
-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ

if _G.AUTO_FARM_V2_ENABLED and _G.AUTO_FARM_ENABLED then
    warn("  ‚ö†Ô∏è WARNING: Both V1 and V2 are enabled! Starting V2 only (Jung Fast)...")
    _G.AUTO_FARM_ENABLED = false
end

if _G.AUTO_FARM_V2_ENABLED then
    print("\n  ‚ö° Starting Auto Farm V2 (Jung Fast)...")
    print(string.format("      Base Delay Reel: %.1fs, Complete: %.1fs",
        _G.JUNG_V2_STATE.baseDelayReel, _G.JUNG_V2_STATE.baseDelayComplete))
    task.wait(2)
    startAutoFarmV2()
    task.wait(1)
    print("  ‚ö° ‚úÖ Auto Farm V2 started")
elseif _G.AUTO_FARM_ENABLED then
    print("\n  üé£ Starting Auto Farm V1...")
    print(string.format("      Success Delay: %.1fs", _G.FARM_SUCCESS_DELAY))
    task.wait(2)
    task.spawn(runMainLoop)
    task.wait(1)
    print("  üé£ ‚úÖ Auto Farm V1 started")
else
    print("\n  ‚ö†Ô∏è No Auto Farm enabled (both V1 and V2 are OFF)")
end

-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
-- STEP 2: START AUTO ARTIFACT
-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ

if _G.AUTO_ARTIFACT_ENABLED then
    print("\n  üóø Starting Auto Artifact...")
    print(string.format("      Skip Temples: [T1:%s, T2:%s, T3:%s, T4:%s]",
        artifactSkipToggles[1] and "‚è≠Ô∏è" or "‚úÖ",
        artifactSkipToggles[2] and "‚è≠Ô∏è" or "‚úÖ",
        artifactSkipToggles[3] and "‚è≠Ô∏è" or "‚úÖ",
        artifactSkipToggles[4] and "‚è≠Ô∏è" or "‚úÖ"))
    task.wait(2)
    startAutoArtifact()
    task.wait(1)
    print("  üóø ‚úÖ Auto Artifact started")
end

-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
-- STEP 3: START AUTO WEATHER
-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ

if autoWeatherConfigEnabled then
    print(string.format("\n  üå§Ô∏è Starting Auto Weather (Delay: %ds)...", _G.WEATHER_PURCHASE_DELAY))
    task.wait(2)
    startAutoWeather()
    task.wait(1)
    print("  üå§Ô∏è ‚úÖ Auto Weather started")
end

-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
-- STEP 4: START AUTO TOTEM
-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ

if _G.AUTO_TOTEM_ENABLED then
    print(string.format("\n  üóø Starting Auto Totem (Cycle: %d min, Item ID: %d)...",
        _G.TOTEM_CYCLE_DELAY, _G.TOTEM_ITEM_ID))
    task.wait(2)
    startAutoTotemLoop()
    task.wait(1)
    print("  üóø ‚úÖ Auto Totem started")
end

-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
-- STEP 5: START AUTO UPGRADE (Rod & Bait)
-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ

if _G.AUTO_UPGRADE_ROD_ENABLED or _G.AUTO_UPGRADE_BAIT_ENABLED then
    print("\n  ‚¨ÜÔ∏è Starting Auto Upgrade System...")
    task.wait(2)

    if _G.AUTO_UPGRADE_ROD_ENABLED then
        print("      ‚¨ÜÔ∏è Auto Upgrade Rod...")
        startAutoUpgradeRod()
        task.wait(1)
    end

    if _G.AUTO_UPGRADE_BAIT_ENABLED then
        print("      ‚¨ÜÔ∏è Auto Upgrade Bait...")
        startAutoUpgradeBait()
        task.wait(1)
    end

    print("  ‚¨ÜÔ∏è ‚úÖ Auto Upgrade started")
end

-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
-- STEP 6: START MISC FEATURES (Freeze, GPU Saver, Animations, etc.)
-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
-- Urutan eksekusi: Freeze Character ‚Üí Disable Animations ‚Üí Remove Islands ‚Üí GPU Saver

print("\n  ‚öôÔ∏è Initializing Misc features...")
task.wait(1)

-- 1. FREEZE CHARACTER (jika enabled)
if _G.FREEZE_CHAR_ENABLED then
    print("      üîí Enabling Freeze Character...")
    task.wait(0.5)
    freezeCharPosition()
    print("      üîí ‚úÖ Freeze Character enabled")
end

-- 2. DISABLE ANIMATIONS (jika enabled)
if _G.DISABLE_ANIMATIONS_ENABLED then
    print("      üé¨ Setting up animation hooks...")
    setupAnimationHook()
    task.wait(0.5)
    deleteAllAnimations()
    print("      üé¨ ‚úÖ Animations disabled")
end

-- 3. REMOVE ISLANDS (jika enabled)
if removeIslands then
    pcall(function()
        local islandsFolder = workspace:FindFirstChild("Islands")
        if islandsFolder then
            local count = 0
            for _, island in pairs(islandsFolder:GetChildren()) do
                island:Destroy()
                count = count + 1
            end
            print(string.format("      üèùÔ∏è ‚úÖ Removed %d islands", count))
        else
            print("      üèùÔ∏è ‚ö†Ô∏è Islands folder not found")
        end
    end)
end

-- 4. GPU SAVER (jika enabled)
if _G.GPU_SAVER_ENABLED then
    print("      üíæ Enabling GPU Saver...")
    enableGPUSaver()
    print("      üíæ ‚úÖ GPU Saver enabled")
end

-- 5. UNLOCK FPS (jika enabled)
if unlockFPS then
    pcall(function()
        setfpscap(240)
        print("      üìä ‚úÖ FPS unlocked to 240")
    end)
end

-- 6. AUTO MEGALODON (jika enabled)
if _G.AUTO_MEGALODON_ENABLED then
    print("      ü¶à Enabling Auto Megalodon...")
    task.wait(0.5)
    setAutoMegalodon(true)
    print("      ü¶à ‚úÖ Auto Megalodon enabled")
end

-- 7. AUTO QUEST (jika enabled)
if _G.AUTO_QUEST_ENABLED then
    print("      üìú Enabling Auto Quest...")
    task.wait(0.5)
    setAutoQuest(true)
    print("      üìú ‚úÖ Auto Quest enabled")
end

print("  ‚öôÔ∏è ‚úÖ Misc features initialized")

-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
-- STEP 7: START WEBHOOK NOTIFICATIONS
-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ

if _G.WEBHOOK_ENABLED and _G.WEBHOOK_SECRET and _G.WEBHOOK_SECRET ~= "" then
    print("\n  üîî Starting Webhook Notifier (SECRET fish detection)...")
    task.wait(2)
    startWebhookNotifier()
    task.wait(1)
    print("  üîî ‚úÖ Webhook Notifier started")
elseif _G.WEBHOOK_ENABLED and (_G.WEBHOOK_SECRET == nil or _G.WEBHOOK_SECRET == "") then
    warn("\n  ‚ö†Ô∏è Webhook enabled but URL is empty! Please set URL in config_loader.lua")
end

-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
-- STEP 8 (FINAL): START AUTO SELL
-- ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
-- Auto Sell dijalankan paling akhir setelah semua fitur berjalan
-- Delay 6 detik untuk memastikan semua sistem sudah stabil

if autoSellConfigEnabled then
    print("\n  ‚è≥ Waiting 6 seconds before starting Auto Sell...")
    print("     (Ensuring all systems are stable)")
    task.wait(6)
    print(string.format("\n  üí∞ Starting Auto Sell (Delay: %ds)...", _G.AUTO_SELL_DELAY))
    startAutoSell()
    task.wait(1)
    print("  üí∞ ‚úÖ Auto Sell started")
end

-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
-- FINAL STARTUP MESSAGE
-- ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê

print("")
print(string.rep("=", 80))
print("‚úÖ AUTO FISHING SYSTEM v7.0 HEADLESS - FULLY LOADED!")
print("‚úÖ All enabled features started successfully!")
print(string.rep("=", 80))
print("")
print("üìã Active Features Summary:")
print(string.format("   üé£ Auto Farm V1: %s", _G.AUTO_FARM_ENABLED and "üü¢ RUNNING" or "‚ö´ OFF"))
